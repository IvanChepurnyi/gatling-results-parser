{
    "name": "Gatling result parser script",
    "require": {
        "symfony/symfony": "@stable",
        "symfony/monolog-bundle": "@stable",
        "vlucas/phpdotenv": "~2.0",
        "php": "^5.6|>=7.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.4",
        "squizlabs/php_codesniffer": "*",
        "phpmd/phpmd": "^2.3",
        "phpspec/nyan-formatters": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Gatling\\ParserBundle\\": "src/Gatling/ParserBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "spec\\Gatling\\ParserBundle\\": "spec/Gatling/ParserBundle"
        }
    },
    "config": {
        "bin-dir": "bin/",
        "optimize-autoloader": true
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Chepurnyi",
            "email": "ivan@ecomdev.org"
        }
    ]
}
